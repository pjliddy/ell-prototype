<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">{{title}}</h3>
  </div>
  <div class="panel-body">
    <ul>
      {{#each links as |link|}}
        <li>
          <a href="{{link.url}}">{{link.label}}: </a>{{link.copy}}
        </li>
      {{/each}}
    </ul>
  </div>
</div>
